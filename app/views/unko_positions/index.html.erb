<h1>Listing unko_positions</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @unko_positions.each do |unko_position| %>
  <tr>
    <td><%= unko_position.latitude %></td>
    <td><%= unko_position.longitude %></td>
    <td><%= unko_position.info %></td>
    <td><%= link_to 'Show', unko_position %></td>
    <td><%= link_to 'Edit', edit_unko_position_path(unko_position) %></td>
    <td><%= link_to 'Destroy', unko_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

